      "settings": {
        "foreground": "#916EA0"
      }
    },

    // ═══════════════════════════════════════════
    // KEYWORDS
    // ═══════════════════════════════════════════
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.instanceof"
      ],
      "settings": {
        "foreground": "#B48246"
      }
    },
    {
      "name": "Storage Keywords",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B48246"
      }
